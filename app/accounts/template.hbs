{{#paper-toolbar}}
  <h2 class="md-toolbar-tools">
    {{#paper-sidenav-toggle class="menu-sidenav-toggle"}}
      {{paper-icon icon="menu"}}
    {{/paper-sidenav-toggle}}
    <span>Accounts</span>
  </h2>
{{/paper-toolbar}}

{{#paper-list}}
  {{#each content as |account|}}
    {{account-list-item account=account editAccount=(action 'editAccount' account)}}
  {{/each}}
{{/paper-list}}

{{#paper-button classNames="md-fab md-primary bottom-right-button" action="createAccount"}} {{paper-icon icon="add" size=2 classNames="patch-size-2"}} {{/paper-button}}

{{outlet}}
